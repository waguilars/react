{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "module": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "outDir": "dist",
    "resolveJsonModule": true,
    "types": ["vite/client"],
    "paths": {
      "@components/*": ["src/components/*"]
    }
  },
  "include": ["src/**/*.js", "src/**/*.jsx"]
}